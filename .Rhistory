require(RCurl)
require(XML)
require(dplyr)
require(tidyr)
require(ggplot2)
setwd("~/Documents/finding-cars/")
options(scipen = 999)
carData <- read.table("data/carData.csv", sep=';', header=T)
carData %>% filter(prices <= 15000, odometer<= 130000) %>%
ggplot(aes(odometer, prices), size = years) + geom_point(aes(size=years)) + geom_smooth() +
facet_grid(. ~ vehicle_type)
library(rpart)
fit <- rpart(prices ~ ., data=carData[1:100, c(2:6)])
fit <- rpart(prices ~ ., data=carData[1:100, c(2:7)])
printcp(fit) # display the results
plotcp(fit) # visualize cross-validation results
summary(fit) # detailed summary of splits
plot(fit, uniform=TRUE,
main="Classification Tree for Kyphosis")
text(fit, use.n=TRUE, all=TRUE, cex=.8)
# create attractive postscript plot of tree
post(fit, file = "c:/tree.ps",
title = "Classification Tree for Kyphosis")
fit <- rpart(prices ~ ., data=carData[, c(2:7)])
printcp(fit) # display the results
plotcp(fit) # visualize cross-validation results
summary(fit) # detailed summary of splits
